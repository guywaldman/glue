{
  "name": "glue",
  "displayName": "glue",
  "description": "Language support and tooling for Glue",
  "repository": {
    "type": "git",
    "url": "https://github.com/guywaldman/glue"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "scripts": {
    "package": "vsce package"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "glue",
        "aliases": [
          "glue",
          "glue"
        ],
        "extensions": [
          "glue"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "glue",
        "scopeName": "source.glue",
        "path": "./syntaxes/glue.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.2"
  }
}
