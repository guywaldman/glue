/// Represents a person.
model Person {
	/// The person's name.
	name: string
	/// The person's age.
	age: int
	/// The person's address.
	address: Address

	/// The person's address.
	model Address {
		/// The street of the address.
		// NOTE: Not case-sensitive.
		street: string
		/// The city of the address.
		city: string
		/// The country code of the address.
		country_code: string
		/// The zipcode of the address.
		zipcode?: string
	}
}
