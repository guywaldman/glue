@endpoint("GET /users/{id}")
endpoint GetUserById {
	request: {
		id: int
	}

	headers: {
		"X-Request-ID": string
	}

	responses: {
		/// A successful response containing the user details.
		200: Response200
		/// Client error response.
		4XX: ResponseError
		/// Server error response.
		5XX: ResponseError
	}
}

model User {
	id: int
	name: string
}