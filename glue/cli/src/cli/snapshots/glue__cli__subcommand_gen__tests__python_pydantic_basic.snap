"""
"""

from pydantic import BaseModel
from pydantic import Field, BaseModel
from enum import StrEnum
from typing import Annotated, List, Optional

class User(BaseModel):
    email: Annotated[str, Field()]
    """The user's email address"""

    id: Annotated[int, Field()]
    """The unique ID of the user"""

    is_active: Annotated[bool, Field()]
    """Whether the user is active"""

    name: Annotated[str, Field()]
    """The user's name"""

class Post(BaseModel):
    class AdditionalPostDetails(BaseModel):
        likes: Annotated[int, Field()]
        """The number of likes the post has received"""
    

    additional_details: Annotated[Optional[AdditionalPostDetails], Field()]
    """Optional additional details about the post"""

    author: Annotated[int | User, Field()]
    """The author of the post Can be either the user ID or the full user object"""

    content: Annotated[str, Field()]
    """The content of the post"""

    id: Annotated[int, Field()]
    """The unique ID of the post"""

    title: Annotated[str, Field()]
    """The title of the post"""
